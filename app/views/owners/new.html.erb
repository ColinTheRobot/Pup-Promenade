<%= form_for(@owner) do |f| %>
  <%= f.label(:first_name) %>
  <%= f.text_field(:first_name) %>
  <%= f.label(:last_name) %>
  <%= f.text_field(:last_name) %>
  <%= f.label(:email) %>
  <%= f.email_field(:email) %>
  <br>
  <%= f.label(:password) %>
  <%= f.password_field(:password) %>
  <%= f.label(:password_confirmation) %>
  <%= f.password_field(:password_confirmation) %>
  <br>
  <%= f.label(:phone) %>
  <%= f.text_field(:phone) %>
  <br>
  <%= f.submit %>

  <% end %>
