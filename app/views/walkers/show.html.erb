<% binding.pry %>

<%= form_for @walker do |f| %>
  <%= f.label(:image_url) %>
  <%= f.text_field(:image_url) %>
    <%= f.fields_for :walker_profile, @walker.build_walker_profile do |profile| %>
    <%= profile.label(:neighborhood_id) %>
    <%= profile.collection_select(:neighborhood_id, @neighborhood, :id, :name) %>
    <%= profile.label(:dog_id) %>
    <%= profile.collection_select(:dog_id, @dogs, :id) %>
  <% end %>
  <br>
  <%= f.label(:bio) %>
  <%= f.text_area(:bio) %>
<% end %>
